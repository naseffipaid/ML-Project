{
  "family": "studentperformance",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "studentperformance",
      "image": "PLACEHOLDER_IMAGE", 
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::585008067217:role/ecsTaskExecutionRole"
}
