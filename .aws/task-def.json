{
  "family": "studentperformance1",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "studentperformance1",
      "image": "585008067217.dkr.ecr.eu-north-1.amazonaws.com/studentperformance1:latest",
      "memory": 3072,
      "cpu": 1024,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/studentperformance1",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ],
  "requiresCompatibilities": ["EC2"],
  "cpu": "1024",
  "memory": "3072",
  "executionRoleArn": "arn:aws:iam::585008067217:role/ecsTaskExecutionRole"
}
